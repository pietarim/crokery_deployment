{
  "name": "grocery-app",
  "version": "1.0.0",
  "description": "Add groceries to shopping list from recipes",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "cross-env NODE_ENV=development ts-node ./src/index.ts",
    "start": "cross-env NODE_ENV=production node ./src/index.js",
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development nodemon",
    "db:local": "docker run -e POSTGRES_PASSWORD=mysecretpassword -p 5432:5432 postgres",
    "db:dev-data": "ts-node src/seeders/devInitQueries.ts",
    "db:rollback": "ts-node src/seeders/rollBackMigrations.ts",
    "db:migrate:prod": "node script/startMigration.js",
    "seed:items:prod": "node script/seedProductionItems.js"
  },
  "author": "Pietari Murtom√§ki",
  "license": "SEE LICENSE IN readme.md",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.0",
    "@types/validator": "^13.11.6",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.35.0",
    "sequelize-typescript": "^2.1.5",
    "umzug": "^3.4.0"
  }
}
