  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "runtimeArgs": ["-r", "ts-node/register"],
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\server\\src\\index.ts",
      "cwd": "${workspaceFolder}\\server",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
